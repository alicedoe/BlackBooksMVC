RewriteEngine on
RewriteCond %{HTTP_HOST} ^(www.)?blackbooks.mvc$
RewriteCond %{REQUEST_URI} !^/indexdir/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /indexdir/$1
RewriteCond %{HTTP_HOST} ^(www.)?blackbooks.mvc$
RewriteRule ^(/)?$ indexdir/index.php [L]
